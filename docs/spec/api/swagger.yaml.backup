basePath: /api/v1
definitions:
  entities.AlertHistory:
    properties:
      alert_type:
        description: offline, error, warning
        type: string
      created_at:
        type: integer
      deleted_at:
        type: integer
      device_group:
        type: string
      device_name:
        type: string
      logname:
        type: string
      message:
        type: string
      resolved_at:
        type: integer
      resolved_by:
        type: string
      severity:
        description: low, medium, high, critical
        type: string
      status:
        description: active, resolved, acknowledged
        type: string
      updated_at:
        type: integer
    type: object
  entities.Device:
    properties:
      created_at:
        type: integer
      deleted_at:
        type: integer
      device_group:
        type: string
      id:
        type: integer
      name:
        type: string
      updated_at:
        type: integer
    type: object
  entities.ElasticsearchMonitor:
    properties:
      alert_threshold:
        type: string
      check_type:
        type: string
      created_at:
        type: integer
      deleted_at:
        type: integer
      description:
        type: string
      enable_auth:
        type: boolean
      enable_monitor:
        type: boolean
      host:
        type: string
      id:
        type: integer
      interval:
        type: integer
      name:
        type: string
      password:
        type: string
      port:
        type: integer
      receivers:
        items:
          type: string
        type: array
      subject:
        type: string
      updated_at:
        type: integer
      username:
        type: string
    type: object
  entities.Index:
    properties:
      created_at:
        type: integer
      deleted_at:
        type: integer
      device_group:
        type: string
      field:
        type: string
      id:
        type: integer
      logname:
        type: string
      pattern:
        type: string
      period:
        type: string
      targets:
        description: TargetID    int    `gorm:"index" json:"target_id" form:"target_id"`
        items:
          $ref: '#/definitions/entities.Target'
        type: array
      unit:
        type: integer
      updated_at:
        type: integer
    type: object
  entities.LoginRequest:
    properties:
      password:
        type: string
      username:
        type: string
    required:
    - password
    - username
    type: object
  entities.LoginResponse:
    properties:
      token:
        type: string
      user:
        $ref: '#/definitions/entities.User'
    type: object
  entities.MainMenu:
    properties:
      icon:
        type: string
      id:
        type: integer
      module:
        type: string
      router_path:
        type: string
      sort:
        type: integer
      title:
        type: string
    type: object
  entities.Module:
    properties:
      created_at:
        type: integer
      deleted_at:
        type: integer
      disabled:
        type: boolean
      id:
        type: integer
      name:
        type: string
      updated_at:
        type: integer
      url:
        type: string
    type: object
  entities.Permission:
    properties:
      action:
        description: e.g., "create", "read", "update", "delete"
        type: string
      created_at:
        type: integer
      deleted_at:
        type: integer
      description:
        type: string
      id:
        type: integer
      name:
        type: string
      resource:
        description: e.g., "device", "target", "user"
        type: string
      updated_at:
        type: integer
    type: object
  entities.Receiver:
    properties:
      created_at:
        type: integer
      deleted_at:
        type: integer
      id:
        type: integer
      name:
        items:
          type: string
        type: array
      updated_at:
        type: integer
    type: object
  entities.Role:
    properties:
      created_at:
        type: integer
      deleted_at:
        type: integer
      description:
        type: string
      id:
        type: integer
      name:
        type: string
      permissions:
        items:
          $ref: '#/definitions/entities.Permission'
        type: array
      updated_at:
        type: integer
    type: object
  entities.Target:
    properties:
      created_at:
        type: integer
      deleted_at:
        type: integer
      enable:
        type: boolean
      id:
        type: integer
      indices:
        description: Indices []Index `gorm:"foreignKey:TargetID;constraint:OnUpdate:RESTRICT,OnDelete:RESTRICT;"`
        items:
          $ref: '#/definitions/entities.Index'
        type: array
      subject:
        type: string
      to:
        items:
          type: string
        type: array
      updated_at:
        type: integer
    type: object
  entities.User:
    properties:
      created_at:
        type: integer
      deleted_at:
        type: integer
      email:
        type: string
      id:
        type: integer
      is_active:
        type: boolean
      role:
        $ref: '#/definitions/entities.Role'
      role_id:
        type: integer
      updated_at:
        type: integer
      username:
        type: string
    type: object
  models.Response:
    properties:
      body: {}
      msg:
        type: string
      success:
        type: boolean
    type: object
